<div class="uspto-header">
    <form name="quizform" action="/result" method="post"> 
    <div class="uspto-header__top">
        <div class="uspto-container uspto-container--fluid">
            <div class="uspto-header__top__button">
                <div class="btn-group">
                <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                    <i class="icon icon-bars"></i><span class="caret"></span>
                </button>
                <ul class="dropdown-menu" role="menu">
                    <li><a href="/">Reset Quiz</a></li>
                    <li><a href="http://www.usptocode.org">C.O.D.E. Homepage</a></li>
                    <li><a href="http://www.facebook.com/usptocode">C.O.D.E. on Facebook</a></li>
                </ul>
                </div>
            </div>
            <h4 class="uspto-header__top__title">What kind of humanitarian are you? C.O.D.E. Quiz</h4>
        </div>
    </div>
    <div class="container-fluid padding-top-4 padding-left-4">
    <!-- Medicine, Nutrition, Sanitation, Household Energy, Living Standards -->
        <h3><span class="label label-primary">Question 1</span> &nbsp;Pick an icon:</h3>
            <div class="btn-group" data-toggle="buttons">
                <label class="btn btn-default">
                    <input type="radio" name="post[icons]" id="medicine" value="medicine">
                    <img width="50" height="50" style="margin: 5px;" src="/images/medicine.svg"></img>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[icons]" id="nutrition" value="nutrition">
                    <img width="50" height="50" style="margin: 5px;" src="/images/nutrition.svg"></img>
                </label>
                 <label class="btn btn-default">
                    <input type="radio" name="post[icons]" id="householdenergy" value="householdenergy">
                    <img width="50" height="50" style="margin: 5px;" src="/images/householdenergy.svg"></img>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[icons]" id="sanitation" value="sanitation">
                    <img width="50" height="50" style="margin: 5px;" src="/images/sanitation.svg"></img>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[icons]" id="livingstandards">   
                    <img width="50" height="50" style="margin: 5px;" src="/images/livingstandards.svg"></img>
                </label>
            </div>
        <h3><span class="label label-primary">Question 2</span>&nbsp;Choose a word:</h3>
            <div class="btn-group" data-toggle="buttons">
                <label class="btn btn-default">
                    <input type="radio" name="post[words]" id="medicineword" value="medicine">
                    <%= medword %>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[words]" id="nutritionword" value="nutrition">
                    <%= nutword %>
                </label>
                 <label class="btn btn-default">
                    <input type="radio" name="post[words]" id="householdenergyword" value="householdenergy">
                    <%= heword %>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[words]" id="sanitationword" value="sanitation">
                    <%= sanword %>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[words]" id="livingstandardsword" value="livingstandards">   
                    <%= livword %>
                </label>
            </div>
       <h3><span class="label label-primary">Question 3</span> &nbsp;Pick a logo:</h3>
            <div class="btn-group" data-toggle="buttons">
                <label class="btn btn-default">
                    <input type="radio" name="post[logos]" value="<%= logocats[0] %>">
                    <img width="50" height="50" style="margin: 5px;" src="<%= logos[0] %>"></img>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[logos]" value="<%= logocats[1] %>">
                    <img width="50" height="50" style="margin: 5px;" src="<%= logos[1] %>"></img>
                </label>
                 <label class="btn btn-default">
                    <input type="radio" name="post[logos]" value="<%= logocats[2] %>">
                    <img width="50" height="50" style="margin: 5px;" src="<%= logos[2] %>"></img>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[logos]" value="<%= logocats[3] %>">
                    <img width="50" height="50" style="margin: 5px;" src="<%= logos[3] %>"></img>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[logos]" value="<%= logocats[4] %>">   
                    <img width="50" height="50" style="margin: 5px;" src="<%= logos[4] %>"></img>
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="post[logos]" value="<%= logocats[5] %>">   
                    <img width="50" height="50" style="margin: 5px;" src="<%= logos[5] %>"></img>
                </label>
            </div>
       <h3><span class="label label-primary">Question 4</span>&nbsp;Choose a title:</h3>
            <div class="btn-group-vertical" data-toggle="buttons">
                <label class="btn btn-default" style="margin-bottom: 4px;white-space: normal;width: 275px;">
                    <input type="radio" name="post[titles]" value="<%= titlecats[0] %>">
                    <%= title[0] %>
                </label>
                <label class="btn btn-default" style="margin-bottom: 4px;white-space: normal;width: 275px;">
                    <input type="radio" name="post[titles]" value="<%= titlecats[1] %>">
                    <%= title[1] %>
                </label>
                 <label class="btn btn-default" style="margin-bottom: 4px;white-space: normal;width: 275px;">
                    <input type="radio" name="post[titles]" value="<%= titlecats[2] %>">
                    <%= title[2] %>
                </label>
                <label class="btn btn-default" style="margin-bottom: 4px;white-space: normal;width: 275px;">
                    <input type="radio" name="post[titles]" value="<%= titlecats[3] %>">
                    <%= title[3] %>
                </label>
                <label class="btn btn-default" style="margin-bottom: 4px;white-space: normal;width: 275px;">
                    <input type="radio" name="post[titles]" value="<%= titlecats[4] %>">   
                    <%= title[4] %>
                </label>
                <label class="btn btn-default" style="margin-bottom: 4px;white-space: normal;width: 275px;">
                    <input type="radio" name="post[titles]" value="<%= titlecats[5] %>">   
                    <%= title[5] %>
                </label>
            </div>
       <h3><span class="label label-primary">Question 5</span>&nbsp;Pick an inventive team:</h3>
            <div class="btn-group-vertical" data-toggle="buttons">
                <label class="btn btn-default" style="text-align: left;margin-bottom:4px;">
                    <input type="radio" name="post[teamtype]" value="independentresearcher">
                    <i class="icon icon-user"></i>&nbsp;&nbsp;&nbsp;Independent researcher
                </label>
                <label class="btn btn-default" style="text-align: left;margin-bottom:4px;">
                    <input type="radio" name="post[teamtype]" value="university">
                    <i class="icon icon-university"></i>&nbsp;&nbsp;&nbsp;University/Research Institution
                </label>
                 <label class="btn btn-default" style="text-align: left;margin-bottom:4px;">
                    <input type="radio" name="post[teamtype]" value="corporation">
                    <i class="icon icon-building-o"></i>&nbsp;&nbsp;&nbsp;Corporation
                </label>
                <label class="btn btn-default" style="text-align: left;margin-bottom:4px;">
                    <input type="radio" name="post[teamtype]" value="nonprofit">
                    <i class="icon icon-users"></i>&nbsp;&nbsp;&nbsp;Non-Profit Organization
                </label>
            </div>
        <br><br><br>
    </div>
    <div class="uspto-header__bottom">
        <div class="uspto-container uspto-container--fluid">
            <ul class="uspto-header__bottom__links uspto-header__bottom__links--right">
                <li><input type="submit" value="submit" class="btn btn-default" style="float: right;"></input></li>
        </div> 
    </div>
    </form>
</div>
